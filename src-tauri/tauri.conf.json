{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Space Usage",
  "version": "0.1.0",
  "identifier": "com.example.space-usage-app",
  "build": {
    "beforeDevCommand": "node scripts/gen-icons.mjs && npm run dev",
    "beforeBuildCommand": "node scripts/gen-icons.mjs && npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Space Usage",
        "width": 1100,
        "height": 760,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "icon": ["icons/icon.png"],
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {}
}

